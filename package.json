{
  "name": "github-badge",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "Arnav K <arnavkaushal09@gmail.com>",
  "homepage": "https://github.com/ArnavK-09/luonutt#readme",
  "license": "MIY",
  "source": "lib/main.ts",
  "main": "dist/main.js",
  "outputFormat": "commonjs",
  "target": "browser",
  "scripts": {
    "check": "tsc --noEmit",
    "start": "tsc lib/main.ts --watch",
    "build": "tsc lib/main.ts --outDir dist",
    "fmt": "npx prettier -w ."
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArnavK-09/luonutt.git"
  },
  "bugs": {
    "url": "https://github.com/ArnavK-09/luonutt/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "prettier": "^3.3.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "iife"
    ],
    "minify": true,
    "outDir": "dist",
    "clean": true,
    "legacyOutput": true,
    "sourcemap": false,
    "treeshake": true,
    "splitting": false,
    "ignoreWatch": [
      "/node_modules/"
    ],
    "dts": false
  }
}
